<body>

    <section class="pricing py-5  "  >
        <div class="container">
            <div class="row justify-content-center align-items-center">
                <!-- Free Tier -->
                <div class="col-lg-4">
                    <div class="card mb-5 mb-lg-0">
                        <div class="card-body">
                            <h5 class="card-title text-muted text-uppercase text-center">Pro License </h5>
                            <h6 class="card-price text-center">$100<span class="period">/month</span></h6>
                            <hr>
                            <p>Get license to start use our Quick Order System and make your busissness grow.</p>
                            @Html.ActionLink("Get Licence", "GetLicencePro", "License", null, new { @class = "btn btn-warning" })

                        </div>
                    </div>
                </div>

                <div class="col-lg-4">
                    <div class="card mb-5 mb-lg-0">
                        <div class="card-body">
                            <h5 class="card-title text-muted text-uppercase text-center"> Trial License </h5>
                            <h6 class="card-price text-center">$0<span class="period">/month</span></h6>
                            <hr>
                            <p>Get license to start use our Quick Order System and make your busissness grow.</p>
                            @Html.ActionLink("Get Licence", "GetLicense", "License", null, new { @class = "btn btn-warning" })

                        </div>
                    </div>
                </div>


            </div>
        </div>
    </section>
